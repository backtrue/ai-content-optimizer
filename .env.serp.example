# SERP API Configuration
# Multiple services for redundancy and cost optimization

# SerpAPI (https://serpapi.com)
SERPAPI_KEY=d5d040588a63fe91e0643b2a7ee1e4a8778ea777d0a9f7e065f77cd05b2a05db
SERPAPI_ENABLED=true

# ValueSERP (https://www.valueserp.com)
VALUESERP_KEY=your_valueserp_api_key_here
VALUESERP_ENABLED=false

# ZenSERP (https://zenserp.com)
ZENSERP_KEY=your_zenserp_api_key_here
ZENSERP_ENABLED=false

# Service rotation strategy
# Options: round-robin, priority, random, fallback
SERP_ROTATION_STRATEGY=fallback

# Priority order (used in priority/fallback modes)
# Format: comma-separated list of service names
SERP_SERVICE_PRIORITY=serpapi,valueserp,zenserp

# Retry configuration
SERP_MAX_RETRIES=3
SERP_RETRY_DELAY_MS=1000

# Rate limiting (requests per minute)
SERP_RATE_LIMIT_RPM=60

# Logging
SERP_DEBUG=false
SERP_LOG_FAILURES=true
